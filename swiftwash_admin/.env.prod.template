# SwiftWash Admin - Production Environment Configuration
# Copy this file to .env.prod and fill in your production values
# DO NOT commit the .env.prod file to version control

# Firebase Configuration (Production)
FIREBASE_API_KEY=your_production_firebase_api_key
FIREBASE_AUTH_DOMAIN=swiftwash-prod.firebaseapp.com
FIREBASE_PROJECT_ID=swiftwash-prod
FIREBASE_STORAGE_BUCKET=swiftwash-prod.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_APP_ID=1:123456789012:web:abcdef123456

# Security Configuration (Production)
ENCRYPTION_KEY=your_32_character_production_encryption_key_here
JWT_SECRET=your_production_jwt_secret_here
SESSION_TIMEOUT_MINUTES=15

# API Configuration (Production)
API_BASE_URL=https://api.swiftwash.com
API_TIMEOUT_SECONDS=15

# Rate Limiting (Production)
MAX_REQUESTS_PER_MINUTE=30
MAX_LOGIN_ATTEMPTS=3

# Feature Flags (Production)
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_DEBUG_LOGGING=false
