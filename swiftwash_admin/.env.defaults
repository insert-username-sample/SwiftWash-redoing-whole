# SwiftWash Admin - Default Environment Configuration
# This file contains default values for development
# DO NOT commit sensitive production values to version control

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=swiftwash-alpha.firebaseapp.com
FIREBASE_PROJECT_ID=swiftwash-alpha
FIREBASE_STORAGE_BUCKET=swiftwash-alpha.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789012
FIREBASE_APP_ID=1:123456789012:web:abcdef123456

# Security Configuration
ENCRYPTION_KEY=development_key_change_in_production_32_chars_min
JWT_SECRET=development_jwt_secret_change_in_production
SESSION_TIMEOUT_MINUTES=30

# API Configuration
API_BASE_URL=https://api.swiftwash.com
API_TIMEOUT_SECONDS=30

# Rate Limiting
MAX_REQUESTS_PER_MINUTE=60
MAX_LOGIN_ATTEMPTS=5

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false
ENABLE_DEBUG_LOGGING=true
